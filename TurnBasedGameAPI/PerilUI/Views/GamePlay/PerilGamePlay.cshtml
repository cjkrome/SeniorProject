                    </form>
                    <p id="purchaseResults" class="redText"></p>

                    <button id="confirmPurchaseTroopsBtn" onclick="AddPurchasedTroops()" type="button" class="btn btn-primary btn-lg" data-dismiss="modal">Confirm</button>
                    <button id="purchaseTroopsBtn" onclick="BuyTroops()" type="button" class="btn btn-primary btn-lg">Purchase</button>
                    <button type="button" class="btn btn-danger btn-lg" data-dismiss="modal">Cancel</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Move Modal Coded by James -->
    <div id="moveModal" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <!-- Modal Content -->
            <div class="modal-content">
                <div class="modal-header text-center">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h4 id="moveModalHeader"></h4>
                </div>
                <div class="modal-body text-left">
                    <p id="moveModalTroopsBody"></p>
                    <p id="moveModalLeaderBody"></p>
                    <p id="insufficientTroopCount" class="redText"></p>
                    <form id="moveTroopsForm">
                        Enter how many troops you would like to move:<br>
                        <input type="number" id="moveTroopsField" min="1" max="1">
                    </form>
                    <p id="invalidTroopEntry" class="redText"></p>
                    <button id="moveLeaderBtn" onclick="MoveLeader()" type="button" class="btn btn-primary btn-lg" data-dismiss="modal">Move Leader</button>
                    <button id="moveTroopsBtn" onclick="MoveTroops()" type="button" class="btn btn-primary btn-lg" data-dismiss="modal">Move Troops</button>
                    <button type="button" class="btn btn-danger btn-lg" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Commit Attacker Troops Modal Coded by James -->
    <div id="commitModal" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <!-- Modal Content -->
            <div class="modal-content">
                <div class="modal-header text-center">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h4 id="commitHeader"></h4>
                </div>
                <div class="modal-body text-left">
                    <p id="commitAttackerTroops"></p>
                    <p id="commitDefenderTroops"></p>
                    <p id="insufficientTroops" class="redText"></p>
                    <form id="commitForm">
                        Enter how many troops you would like to commit to battle:<br>
                        <input type="number" id="commitTroopField" min="1" max="1">
                    </form>
                    <p id="invalidTroopEntry" class="redText"></p>
                    <button id="commitBtn" onclick="ValidateTroopCount()" type="button" class="btn btn-primary btn-lg">Commit</button>
                    <button id="OpenAttackBtn" onclick="InitBattleValues()" type="button" class="btn btn-primary btn-lg" data-toggle="modal" data-target="#battleViewModal" data-dismiss="modal">Begin Attack</button>
                    <button type="button" class="btn btn-danger btn-lg" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Battle View Modal Coded by James -->
    <div id="battleViewModal" class="modal fade" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content -->
            <div class="modal-content">
                <div class="modal-header text-center">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h3 id="attackVsDefend"></h3>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-6 text-left">
                            <h4>Attacker</h4>
                        </div>

                        <div class="col-md-6 text-left">
                            <h4>Defender</h4>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6 text-left">
                            <h5 id="commitedForces"></h5>
                        </div>

                        <div class="col-md-6 text-left">
                            <h5 id="defendingForces"></h5>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-5">
                            <img id="attack3Img" class="skeleton" src="~/Content/Skeleton-Attack.png" alt="Skeleton with a sword" style="width:60px;height:110px;">
                            <img id="xAttack3Img" class="skeletonX" src="~/Content/RedX.png" alt="Crossed out image" style="width:60px;height:110px;">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-5">
                            <img id="attack2Img" class="skeleton" src="~/Content/Skeleton-Attack.png" alt="Skeleton with a sword" style="width:60px;height:110px;">
                            <img id="xAttack2Img" class="skeletonX" src="~/Content/RedX.png" alt="Crossed out image" style="width:60px;height:110px;">
                        </div>

                        <div class="col-md-7">
                            <img id="defend2Img" class="skeleton" src="~/Content/Skeleton-Defend.png" alt="Skeleton with a bow" style="width:60px;height:110px;">
                            <img id="xDefend2Img" class="skeletonX" src="~/Content/RedX.png" alt="Crossed out image" style="width:60px;height:110px;">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-5">
                            <img id="attack1Img" class="skeleton" src="~/Content/Skeleton-Attack.png" alt="Skeleton with a sword" style="width:60px;height:110px;">
                            <img id="xAttack1Img" class="skeletonX" src="~/Content/RedX.png" alt="Crossed out image" style="width:60px;height:110px;">
                        </div>

                        <div class="col-md-7">
                            <img id="defend1Img" class="skeleton" src="~/Content/Skeleton-Defend.png" alt="Skeleton with a bow" style="width:60px;height:110px;">
                            <img id="xDefend1Img" class="skeletonX" src="~/Content/RedX.png" alt="Crossed out image" style="width:60px;height:110px;">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6 text-left">
                            <h5 id="attackTroopsLost"></h5>
                        </div>
                        <div class="col-md-6 text-left">
                            <h5 id="defendTroopsLost"></h5>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6 text-left">
                            <h5 id="attackTroopsRemain"></h5>
                        </div>
                        <div class="col-md-6 text-left">
                            <h5 id="defendTroopsRemain"></h5>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button id="battleResultsBtn" onclick="BattleResults()" class="btn btn-success btn-lg" data-toggle="modal" data-target="#battleResultsModal" data-dismiss="modal">Results</button>
                    <button id="battleViewAttackBtn" onclick="HandleAttacks()" type="button" class="btn btn-primary btn-lg">Attack</button>
                    <button id="battleViewRetreatBtn" onclick="BattleRetreat()" type="button" class="btn btn-primary btn-lg" data-toggle="modal" data-target="#battleResultsModal" data-dismiss="modal">Retreat</button>
                    <button type="button" class="btn btn-danger btn-lg" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Battle Results modal Coded by James -->
    <div id="battleResultsModal" class="modal fade backColor" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content -->
            <div class="modal-content">
                <div class="modal-header text-center">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h3 id="batleRsltHeader"></h3>
                </div>
                <div class="modal-body text-center">
                    <div class="row">
                        <p id="batRsltAttacker"></p>
                        <p id="batRsltDefender"></p>
                    </div>
                    <div class="row">
                        <div class="col-md-5">
                            <img id="resNecroImg" src="~/Content/Necromancer.jpg" alt="Necromancer" style="width:150px;height:225px;">
                        </div>
                        <div class="col-md-3">
                            <img id="resSkeletonImg" src="~/Content/Skeleton-Attack.png" alt="Skeleton with a sword" style="width:100px;height:200px;">
                        </div>
                    </div>
                    <div class="row">
                        <p id="batRsltRes"></p>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger btn-lg" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <!-- gameID, toID, fromID added by James to hold information needed for API calls -->
    <div class="hidden" id="gameID"></div>
    <div class="hidden" id="toID"></div>
    <div class="hidden" id="fromID"></div>
    <!-- 1: Buy leader. -1 Not buying leader-->
    <div class="hidden" id="buyLeader"></div>
</body>

</html>

